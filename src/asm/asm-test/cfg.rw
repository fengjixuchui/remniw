// RUN: %remniw %s -o %t ; echo -10 | lli %t | FileCheck %s --check-prefix=CHECK-N
// RUN: %remniw %s -o %t -debug ; echo -10 | lli %t | FileCheck %s --check-prefix=CHECK-N
