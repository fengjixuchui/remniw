func inc(i int) int { return i+1; }
func dec(j int) int { return j-1; }
func ide(k int) int { return k; }

func foo(n int) int {
    var r int;
    if (n==0) { r=dec(n); }
    r = inc(n);
    return r;
}

func main() int {
    var x,y int;
    x = input;
    if (x>0) { y = foo(x); } else { y = foo(x); }
    return y;
}