// RUN: %remniw %s -o %t ; echo 10 | lli %t | FileCheck %s
// RUN: %remniw %s -o %t -debug ; echo 10 | lli %t | FileCheck %s

func rec(n int) int {
    var f int;
    if (n==0) { f=1; }
    else { f=n*rec(n-1); }
    return f;
}

func main() int {
    var n int;
    n = input;
    output rec(n);
    return 0;
}

